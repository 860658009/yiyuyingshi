<view class="{{[theme+'-page']}}"><uni-bar vue-id="50cad900-1" Max="{{barMax}}" theme="{{theme}}" bind:__l="__l" vue-slots="{{['default']}}"><view style="{{(barStyle)}}"></view><view class="barInput" style="{{(searchView)}}"><input class="barInput-input" confirm-type="search" type="text" placeholder="  输入搜索关键词" data-event-opts="{{[['input',[['input',['$event']]]],['confirm',[['confirm',['$event']]]],['navbar',[['navbar',['$event']]]]]}}" value="{{keyword}}" bindinput="__e" bindconfirm="__e" bindnavbar="__e"/><text data-event-opts="{{[['tap',[['searchIcon',['$event']]]]]}}" class="cuIcon" style="font-weight:900;font-size:45rpx;color:#DD524D;" bindtap="__e"></text></view><view class="{{['carousel-by',theme+'-bg1']}}"></view></uni-bar><view><block wx:if="{{nokeyword}}"><view class="{{['hotkeyword',theme+'-title-color']}}"><block wx:if="{{history.length>0}}"><view class="hottitle"><text class="cuIcon" style="font-size:45rpx;color:#ff8080;"></text><text class="icon" style="font-size:35rpx;">历史搜索</text></view></block><block wx:if="{{history.length>0}}"><view class="history-div"><block wx:for="{{history}}" wx:for-item="list" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['hotkeywordtab',['$0'],[[['history','',index]]]]]]]}}" class="{{['history-data',theme+'-bg3']}}" bindtap="__e"><view class="history-title"><text class="history-title-title">{{list}}</text></view></view></block></view></block><view class="hottitle"><text class="cuIcon" style="font-size:45rpx;color:#ff8080;"></text><text class="hottitle-title " style="font-size:35rpx;">热门搜索</text></view><view class="hotkeyword-div"><block wx:for="{{hotkeywordlist}}" wx:for-item="list" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['hotkeywordtab',['$0'],[[['hotkeywordlist','',index,'title']]]]]]]}}" class="{{['hotkeyword-data',theme+'-bg3']}}" bindtap="__e"><text class="hotkeyword-title">{{list.title}}</text></view></block></view></view></block><block wx:if="{{!nokeyword}}"><view class="vodlist"><block wx:for="{{vodlistdata}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view><block wx:if="{{index==2&&wxAdId.ysId}}"><custom-ad vue-id="{{'50cad900-2-'+index}}" ysId="{{wxAdId.ysId}}" bind:__l="__l"></custom-ad></block><shuvodlist vue-id="{{'50cad900-3-'+index}}" item="{{item}}" theme="{{theme}}" bind:__l="__l"></shuvodlist></view></block></view></block></view><block wx:if="{{noselection}}"><view class="selection"><block wx:for="{{selection}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="selection-item"><view data-event-opts="{{[['tap',[['selectionTab',['$0'],[[['selection','',index,'name']]]]]]]}}" class="selection-data" bindtap="__e"><text class="selection-name">{{item.name}}</text><text class="cuIcon" style="font-size:35rpx;color:#666666;"></text></view></view></block></view></block></view>